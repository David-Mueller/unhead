{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "declaration": true,
    "resolveJsonModule": true,
    "rootDir": ".",
    "baseUrl": ".",
    "jsx": "preserve",
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noUnusedLocals": true,
    "paths": {
      "unhead": ["packages/unhead/src"],
      "@unhead/dom": ["packages/dom/src"],
      "@unhead/ssr": ["packages/ssr/src"],
      "@unhead/schema-org": ["packages/schema-org/src"],
      "@unhead/vue/polyfill": ["packages/vue/src/VueUseHeadPolyfill.ts"],
      "@unhead/vue": ["packages/vue/src"],
      "@unhead/addons": ["packages/addons/src"],
      "@unhead/addons/*": ["packages/addons/*"],
      "unhead/*": ["packages/unhead/*"],
      "@unhead/schema": ["packages/schema/src"],
      "@unhead/schema/*": ["packages/schema/*"],
      "@unhead/shared": ["packages/shared/src"],
      "@unhead/shared/*": ["packages/shared/*"],
      "@unhead/schema-org/*": ["packages/schema-org/*"]
    }
  }
}
